<!DOCTYPE html>
<html>
<head>
  <title>URLockBox</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <% if current_user %>
      "Welcome <%= current_user.email %>" <%= button_to "Log Out", logout_path, method: :get %>
    <% else %>
      <%= button_to "Sign Up", signup_path, method: :get %>
    <% end %>
  </div>
  <hr/>
  <div class="container">
    <%= yield %>
  </div>
</body>
</html>
